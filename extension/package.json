{
  "name": "plinth-extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx webflow extension serve\" \"npm run watch-webpack\"",
    "build": "npm run build-webpack && npx webflow extension bundle",
    "watch-webpack": "webpack --config webpack.config.mjs --mode development --watch",
    "build-webpack": "webpack --config webpack.config.mjs --mode production",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@webflow/designer-extension-typings": "^2.0.2",
    "@webflow/webflow-cli": "^1.12.2",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.5.7",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
